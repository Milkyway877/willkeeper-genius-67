
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "types": ["react", "react-dom"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "jsxImportSource": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true
  }
}
