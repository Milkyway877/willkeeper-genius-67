
{
  "version": 2,
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*).png", "dest": "/$1.png" },
    { "src": "/(.*).jpg", "dest": "/$1.jpg" },
    { "src": "/(.*).ico", "dest": "/public/$1.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}
